                return value.toString();
              }else if (value instanceof String)
              {
                    return value.toString();
              }else if (value instanceof Boolean)
              {
                    return value.toString();
              }else if (value instanceof JSONArray)
              {
                  JSONArray arr = (JSONArray)value;
